<div class="panel has-nested-panel">
  <div class="panel-heading">
    <div class="collapse-panel-title">
      <a data-toggle="collapse" href="#{{panelId}}" aria-expanded="true" aria-controls="collapseOne">
        {{panelName}}
        <i class="fa fa-chevron-down pull-right"></i>
      </a>
    </div>
  </div>
  <div class="panel-body">
    <div class="collapse in" id={{panelId}}>
      {{yield}}
    </div>
  </div>
</div>